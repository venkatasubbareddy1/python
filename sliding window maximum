class Solution(object):
    def maxSlidingWindow(self, nums, k):
        res = []
        sliding = deque()
        for i in range(len(nums)):
            if sliding and sliding[0] < i - k + 1:
                sliding.popleft()
            while sliding and nums[sliding[-1]] < nums[i]:
                sliding.pop()
            sliding.append(i)
            if i >= k - 1:
                res.append(nums[sliding[0]])
        return res
