class Solution(object):
    def detectCycle(self, head):
        if not head:
            return 
        slow=head
        fast=head

        while fast and fast.next:
            slow = slow.next
            fast = fast.next.next
            if slow == fast:
                break

        if not fast or not fast.next:
            return 
        slow = head
        while slow != fast:
            slow = slow.next
            fast = fast.next
        return fast
